{
  "name": "@windingtree/org.id-utils",
  "version": "0.3.2",
  "description": "Shared ORGiD utilities",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig":{
    "access": "public"
  },
  "scripts": {
    "lint": "npx eslint . --ext .ts",
    "lint:fix": "npx eslint . --ext .ts --fix",
    "build": "rm -rf dist && npx tsc -p tsconfig-build.json",
    "build:docs": "rm -rf docs && npx typedoc --plugin typedoc-plugin-markdown --readme none --out docs src/index.ts",
    "test": "npx mocha -r ts-node/register --extension spec.ts",
    "test:coverage": "npx nyc mocha -r ts-node/register --extension spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/windingtree/org.id-sdk.git"
  },
  "keywords": [
    "orgid",
    "windingtree",
    "utilities",
    "typescript"
  ],
  "author": "Winding Tree Developers <dev@windingtree.com>",
  "contributors": [
    "Kostiantyn Smyrnov <kostysh@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/windingtree/org.id-sdk/issues"
  },
  "homepage": "https://github.com/windingtree/org.id-sdk/tree/master/packages/shared#readme",
  "dependencies": {
    "ethers": "5.4.7",
    "axios": "0.21.4",
    "ajv": "8.6.3",
    "ajv-formats": "2.1.1"
  },
  "devDependencies": {
    "eslint": "7.32.0",
    "@typescript-eslint/eslint-plugin": "4.32.0",
    "@typescript-eslint/parser": "4.32.0",
    "typedoc": "0.22.4",
    "typedoc-plugin-markdown": "3.11.2",
    "typescript": "4.4.3",
    "@windingtree/org.id-test-http-server": "0.1.4",
    "@windingtree/org.json-schema": "0.5.11-beta",
    "@types/mocha": "9.0.0",
    "@types/chai": "4.2.22",
    "@types/chai-as-promised": "7.1.4",
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "ts-node": "10.2.1",
    "mocha": "9.1.2",
    "chai": "4.3.4",
    "chai-as-promised": "7.1.1",
    "nyc": "15.1.0"
  }
}
